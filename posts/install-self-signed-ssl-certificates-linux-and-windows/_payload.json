ress ",{"type":22,"tag":92,"props":204,"children":205},{},[206],{"type":27,"value":207},"Get Certificate",{"type":27,"value":164},{"type":22,"tag":92,"props":210,"children":211},{},[212],{"type":27,"value":213},"View",{"type":27,"value":171},{"type":22,"tag":92,"props":216,"children":217},{},[218],{"type":27,"value":117},{"type":27,"value":171},{"type":22,"tag":92,"props":221,"children":222},{},[223],{"type":27,"value":224},"Export",{"type":27,"value":226},". Now choose a filename, and press ",{"type":22,"tag":92,"props":228,"children":229},{},[230],{"type":27,"value":231},"Save",{"type":27,"value":140},{"type":22,"tag":52,"props":234,"children":236},{"id":235},"google-chrome",[237],{"type":27,"value":238},"\n    Google Chrome",{"type":22,"tag":23,"props":240,"children":241},{},[242,243],{"type":27,"value":62},{"type":22,"tag":64,"props":244,"children":247},{"className":245,"href":246,"target":69},[67],"/sites/default/files/Chrome-CertError.png?width=800&height=500&iframe=true",[248],{"type":22,"tag":72,"props":249,"children":252},{"alt":250,"src":251,"style":76},"Google Chrome certificate error.","/sites/default/files/thumb_Chrome-CertError.png",[],{"type":22,"tag":23,"props":254,"children":255},{},[256,257,262,264,269,271,276,278,282],{"type":27,"value":82},{"type":22,"tag":64,"props":258,"children":259},{"href":85,"target":69},[260],{"type":27,"value":261},"the site",{"type":27,"value":263},", and Google Chrome will give you the above warning page. From here, click ",{"type":22,"tag":92,"props":265,"children":266},{},[267],{"type":27,"value":268},"Proceed anyway",{"type":27,"value":270},", and it will take you to the site. Now you need to click on the crossed out lock icon to the left of the address bar, and a popup will appear, from which, you should click ",{"type":22,"tag":92,"props":272,"children":273},{},[274],{"type":27,"value":275},"Certificate Information",{"type":27,"value":277},". Now select Details, then ",{"type":22,"tag":92,"props":279,"children":280